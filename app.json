{
  "expo": {
    "name": "Bus-Tracker-CiudBolivar",
    "slug": "Bus-Tracker-CiudBolivar",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/img/icono.png",
    "scheme": "bustrackerciudbolivar",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/img/icono.png",
        "backgroundImage": "./assets/img/icono.png",
        "monochromeImage": "./assets/img/icono.png",
        "config":{
          "googleMaps":{
            "apiKey":""
          }
        }
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow $(PRODUCT_NAME) to use your location."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}
